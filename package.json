{
  "name": "starter-kit",
  "version": "0.0.1",
  "private": true,
  "devDependencies": {
    "react-scripts": "0.2.1"
  },
  "dependencies": {
    "node-sass": "^3.8.0",
    "react": "^15.2.1",
    "react-dom": "^15.2.1",
    "react-redux": "^4.4.5",
    "react-router": "^2.6.1",
    "redux": "^3.5.2",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.1.0"
  },
  "scripts": {
    "start": "npm run scss && react-scripts start",
    "build": "npm run scss:watch && react-scripts build",
    "eject": "react-scripts eject",
    "scss": "node-sass ./src/styles/app.scss -o ./src/dist/css",
    "scss:watch": "npm run scss && node-sass ./src/styles/app.scss -w -o ./src/dist/css"
  },
  "eslintConfig": {
    "extends": "./node_modules/react-scripts/config/eslint.js"
  }
}
