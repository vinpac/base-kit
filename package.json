{
  "name": "base-kit",
  "version": "0.0.1",
  "private": true,
  "homepage": "http://vinpac.github.io/base-kit",
  "devDependencies": {
    "node-sass": "^3.9.3",
    "react-scripts": "^0.5.1"
  },
  "dependencies": {
    "base-style": "vinpac/base-style",
    "classnames": "^2.2.5",
    "create-react-app": "^0.5.0",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.5",
    "react-router": "^2.8.0",
    "redux": "^3.6.0",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.1.0"
  },
  "scripts": {
    "start": "npm run scss && react-scripts start",
    "build": "npm run scss && react-scripts build",
    "eject": "react-scripts eject",
    "scss": "node-sass ./src/styles/app.scss -o ./src/dist/css",
    "scss:watch": "npm run scss && node-sass ./src/styles/app.scss -w -o ./src/dist/css"
  },
  "eslintConfig": {
    "extends": "./node_modules/react-scripts/config/eslint.js"
  }
}
